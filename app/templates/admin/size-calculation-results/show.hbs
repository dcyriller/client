<div class="admin-build-result">
  <h1>
    Size Calculation #{{this.sizeCalculationResult.id}}
    (<span class="test-addon-name"><LinkTo
        @route="addons.show"
        @model={{this.addon.name}}
      >{{this.addon.name}}</LinkTo></span>
    <span class="test-addon-version">{{this.addonVersion.version}}</span>)
  </h1>

  <div>
    Status:
    <span class="test-build-status">{{this.buildStatus}}</span>
    {{#if this.canRetryBuild}}
      <button
        type="button"
        class="test-retry-build"
        {{on "click" this.retryBuild}}
      >Retry</button>
    {{/if}}
  </div>

  <div>
    Tests run at:
    <span class="test-run-date">{{moment-format
        this.sizeCalculationResult.testsRunAt
        "YYYY-MM-DD HH:mm"
        allowEmpty=true
      }}</span>
  </div>

  <h2>Output</h2>
  <BuildResultOutput @buildResult={{this.sizeCalculationResult}} />
</div>
